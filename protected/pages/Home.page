                                        ID="nombreequipob"
                                        DataField="nombre_equipo_b"
                                        ItemStyle.HorizontalAlign="Center"
                                        HeaderText="Equipo 2"/>
                                    <com:XActiveBoundColumn
                                        ID="resultado"
                                        DataField="resultado"
                                        ItemStyle.HorizontalAlign="Center"
                                        HeaderText="Resultado"/>
